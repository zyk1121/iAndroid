<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhangyuanke.helloandroid">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Module.HA1UI.HA1UIActivity"
            android:label="@string/title_activity_ha1_ui"
            android:parentActivityName=".Tools.BaseActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.zhangyuanke.helloandroid.Tools.BaseActivity" />
        </activity>
        <activity
            android:name=".Module.HA1UI.HA1Layout"
            android:label="@string/title_activity_ha1_layout"
            android:parentActivityName=".Tools.BaseActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.zhangyuanke.helloandroid.Tools.BaseActivity" />
        </activity>
    </application>

</manifest>